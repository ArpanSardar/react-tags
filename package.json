{
  "name": "react-tag-autocomplete",
  "version": "5.8.0",
  "description": "React Tag Autocomplete is a simple tagging component ready to drop in your React projects.",
  "main": "dist/ReactTags.cjs.js",
  "module": "dist/ReactTags.esm.js",
  "scripts": {
    "prepare": "npm run build",
    "prepublish": "npm run build",
    "pretest": "npm run lint && npm run build",
    "lint": "standard lib/*.js spec/*.js",
    "test": "jasmine",
    "coverage": "istanbul cover -i 'dist/**' jasmine",
    "dev": "NODE_ENV=development rollup -c example/rollup.config.js --watch",
    "example": "NODE_ENV=production rollup -c example/rollup.config.js",
    "build": "rollup -c rollup.config.js"
  },
  "files": [
    "lib",
    "dist",
    "example",
    "LICENSE",
    "README.md"
  ],
  "keywords": [
    "react",
    "tags",
    "tag input",
    "react-component",
    "autosuggest",
    "autocomplete"
  ],
  "author": "Matt Hinchliffe",
  "contributors": [
    "Prakhar Srivastav",
    "Simon Hötten",
    "Andre-John Mas",
    "Mike Kamermans",
    "Juliette Prétot",
    "Andrew Pillsbury",
    "Axel Niklasson"
  ],
  "license": "MIT",
  "repository": "https://github.com/i-like-robots/react-tags",
  "peerDependencies": {
    "prop-types": "^15.6.0",
    "react": "^16.3.0",
    "react-dom": "^16.3.0"
  },
  "devDependencies": {
    "coveralls": "^3.0.0",
    "istanbul": "^0.4.4",
    "jasmine": "^3.1.0",
    "jsdom": "^11.10.0",
    "prop-types": "^15.6.0",
    "react": "^16.3.0",
    "react-dom": "^16.3.0",
    "rollup": "^1.0.0",
    "rollup-plugin-buble": "^0.19.6",
    "rollup-plugin-commonjs": "^9.2.0",
    "rollup-plugin-node-resolve": "^4.0.0",
    "rollup-plugin-replace": "^2.1.0",
    "rollup-plugin-serve": "^0.6.1",
    "rollup-plugin-uglify": "^6.0.0",
    "sinon": "^6.1.0",
    "standard": "^11.0.0"
  },
  "engines": {
    "node": ">= 8.15.0"
  }
}
