{
  "name": "react-tag-autocomplete",
  "version": "5.7.1",
  "description": "React Tag Autocomplete is a simple tagging component ready to drop in your React projects.",
  "main": "dist/ReactTags.cjs.js",
  "module": "dist/ReactTags.esm.js",
  "scripts": {
    "prepare": "npm run build",
    "prepublish": "npm run build",
    "pretest": "npm run lint && npm run build",
    "test": "jasmine",
    "coverage": "istanbul cover -i 'dist/**' jasmine",
    "lint": "standard lib/*.js spec/*.js",
    "dev": "webpack-dev-server --progress --env.NODE_ENV=development",
    "example": "webpack -p",
    "build": "rollup -c rollup.config.js"
  },
  "files": [
    "lib",
    "dist",
    "example",
    "LICENSE",
    "README.md"
  ],
  "keywords": [
    "react",
    "tags",
    "tag input",
    "react-component",
    "autosuggest",
    "autocomplete"
  ],
  "author": "Matt Hinchliffe",
  "contributors": [
    "Prakhar Srivastav",
    "Simon Hötten",
    "Andre-John Mas",
    "Mike Kamermans",
    "Juliette Prétot",
    "Andrew Pillsbury"
  ],
  "license": "MIT",
  "repository": "https://github.com/i-like-robots/react-tags",
  "peerDependencies": {
    "prop-types": "^15.6.0",
    "react": "^16.0.0",
    "react-dom": "^16.0.0"
  },
  "devDependencies": {
    "buble": "^0.19.0",
    "buble-loader": "^0.5.0",
    "coveralls": "^3.0.0",
    "istanbul": "^0.4.4",
    "jasmine": "^3.1.0",
    "jsdom": "^11.10.0",
    "prop-types": "^15.6.0",
    "react": "^16.0.0",
    "react-dom": "^16.0.0",
    "rollup": "^0.62.0",
    "rollup-plugin-buble": "^0.19.0",
    "sinon": "^6.1.0",
    "standard": "^11.0.0",
    "webpack": "^3.11.0",
    "webpack-dev-server": "^2.11.0"
  }
}
